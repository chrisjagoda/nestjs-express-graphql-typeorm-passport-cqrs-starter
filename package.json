{
  "name": "nestjs-express-graphql-typeorm-passport-cqrs-starter",
  "version": "1.0.0",
  "description": "NestJS express grapqhl typeorm passport cqrs starter repository",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.11.7",
    "@nestjs/core": "^6.11.7",
    "@nestjs/cqrs": "^6.1.0",
    "@nestjs/graphql": "^6.5.4",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.11.7",
    "@nestjs/typeorm": "^6.3.1",
    "apollo-server-express": "^2.10.1",
    "bcrypt": "^3.0.8",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.9.1",
    "dotenv": "^8.2.0",
    "graphql": "^14.6.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^4.0.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.18.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.7.1",
    "role-acl": "3.3.7",
    "rxjs": "^6.5.4",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.22"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.11.7",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.2",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.17.15",
    "@types/supertest": "^2.0.8",
    "concurrently": "^4.1.2",
    "jest": "^23.6.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5",
    "wait-on": "^3.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
